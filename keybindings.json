[
  {
    "key": "ctrl+shift+m",
    "command": "-editor.action.toggleTabFocusMode"
  },
  {
    "key": "cmd+e cmd+q",
    "command": "SQLTools.newSqlFile"
  },
  {
    "key": "cmd+e cmd+n",
    "command": "SQLTools.openAddConnectionScreen"
  },
  {
    "key": "cmd+e cmd+c",
    "command": "SQLTools.selectConnection"
  },
  {
    "key": "cmd+e cmd+d",
